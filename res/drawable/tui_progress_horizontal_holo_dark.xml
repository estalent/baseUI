<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background" android:drawable="@drawable/tui_progressbar_bg" />
    <item android:id="@*android:id/secondaryProgress" android:drawable="@drawable/tui_progressbar_loaded" />
    <item android:id="@*android:id/progress">
        <selector>
            <item android:state_focused="true" android:drawable="@drawable/tui_progressbar_focus" />
            <item android:drawable="@drawable/tui_progressbar_normal" />
        </selector>
    </item>
</layer-list>